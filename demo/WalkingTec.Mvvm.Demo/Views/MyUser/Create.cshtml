@model WalkingTec.Mvvm.Demo.ViewModels.MyUserVMs.MyUserVM

<wt:form vm="@Model">
  <wt:row items-per-row="ItemsPerRowEnum.Two">
    <wt:textbox field="Entity.Extra1" />
    <wt:textbox field="Entity.Extra2" />
    <wt:textbox field="Entity.ITCode" />
    <wt:textbox field="Entity.Password" />
    <wt:textbox field="Entity.Email" />
    <wt:textbox field="Entity.Name" />
    <wt:combobox field="Entity.Sex" />
    <wt:textbox field="Entity.CellPhone" />
    <wt:upload field="Entity.PhotoId" />
    <wt:combobox field="Entity.IsValid" />
    <wt:selector list-vm="RoleListVM" field="SelectedUserRolesIDs" text-bind="RoleListVM.EntityList[0].RoleName">
      <wt:searchpanel vm="RoleListVM">
        <wt:row items-per-row=" ItemsPerRowEnum.Two">
          <wt:textbox field="RoleListVM.Searcher.RoleCode" />
          <wt:textbox field="RoleListVM.Searcher.RoleName" />
        </wt:row>
      </wt:searchpanel>
    </wt:selector>
  </wt:row>
  <wt:row align="AlignEnum.Right">
    <wt:submitbutton />
    <wt:closebutton />
  </wt:row>
</wt:form>
